        <TextField fx:id="logowanie_login" layoutX="149.0" layoutY="267.0" prefHeight="19.0" prefWidth="324.0">
            <font>
                <Font size="13.0" />
            </font>
        </TextField>
        <Label layoutX="147.0" layoutY="382.0" prefHeight="27.0" prefWidth="102.0" text="Hasło">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <PasswordField fx:id="logowanie_haslo" layoutX="149.0" layoutY="409.0" prefHeight="26.0" prefWidth="324.0">
