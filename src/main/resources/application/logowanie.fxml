        <Button fx:id="btn_zaloguj" layoutX="23.0" layoutY="651.0" mnemonicParsing="false" onAction="#zaloguj_btn_M" prefHeight="40.0" prefWidth="279.0" style="-fx-background-color: #ff8b35;" stylesheets="@style.css" text="Zaloguj" textFill="WHITE">
